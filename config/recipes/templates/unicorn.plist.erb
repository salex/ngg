<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/opt/ruby/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
	</dict>
	<key>Label</key>
	<string>apps.<%= application %>.unicorn</string>
	<key>KeepAlive</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
        <string>/usr/local/opt/ruby/bin/unicorn</string>
        <string>-c</string>
        <string>/Users/developer/apps/<%= application %>/shared/config/unicorn.rb</string>
        <string>-E</string>
        <string>production</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
