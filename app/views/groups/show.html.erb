<fieldset>
  <%= render "group_pref" %>
</fieldset>

<div class="action">
  <%= link_to 'Edit', edit_group_path(@group), :class => "edit-button"  if (can? :update, @group) &&  is_my_group?(@group.id) %>
  <%= link_to 'Destroy', @group, confirm: 'Are you sure?', method: :delete, :class => "destroy-button"  if can? :destroy, @group %></span>

  <%= link_to 'Back', groups_path, :class => "breadcrumb" %>
</div>